{
  "name": "@ursajs/cli",
  "version": "0.0.2",
  "description": "ursa cli",
  "author": "",
  "license": "ISC",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "bin": {
    "ursa": "./bin/index.js",
    "ursajs": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "init": "ts-node ./src/init.ts",
    "test": "mocha test",
    "fix": "esw src --fix --ext .ts",
    "lint": "esw src --clear --ext .ts",
    "lint-w": "esw src --clear -w --ext .ts",
    "build-w": "tsc -w --inlineSourceMap",
    "start": "run-p lint-w build-w",
    "prebuild": "esw src --ext .ts",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "commander": "^3.0.2",
    "consolidate": "^0.15.1",
    "ejs": "^2.7.1",
    "inquirer": "^7.0.0",
    "metalsmith": "^2.3.0",
    "ora": "^4.0.2",
    "simple-git": "^1.129.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^10.14.9",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-watch": "^4.0.2",
    "install": "^0.13.0",
    "npm": "^6.13.6",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ursajs/Ursa.git"
  },
  "bugs": {
    "url": "https://github.com/Ursajs/Ursa/issues"
  },
  "homepage": "https://github.com/Ursajs/Ursa#readme"
}
